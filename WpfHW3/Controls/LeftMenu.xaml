<UserControl x:Class="WpfHW3.Controls.LeftMenu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfHW3.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="40" d:DesignWidth="400"
             Loaded="UserControl_Loaded">
    <StackPanel Margin="5 5 5 5"
        VerticalAlignment="Center"
        HorizontalAlignment="Left"
        Orientation="Horizontal">
        
<!--Source="Csharp.png" Width="30"-->
        <Image x:Name="ProjectImage" 
               Source ="Csharp.png" 
               Width="30"
            />
        
        <Label x:Name="ProjectName"
               Foreground="#E4E4E4" 
               Content="LessonProject1"/>


        <Label x:Name="DataAndTime"
               Foreground="#E4E4E4" 
               Margin="100 0 0 0"
               Content="29.11.2014           18:26"/>

    </StackPanel>
</UserControl>
